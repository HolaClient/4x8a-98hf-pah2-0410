    <meta property="og:url" content="<%= settings.api.client.oauth2.link + "/" %>">
    <meta property="og:type" content="dashboard">
    <meta property="og:title" content="<%= settings.name %>">
    <meta property="og:description" content="Create your own server now at <%= settings.name %> | Powered by HolaClient.">
    <meta property="og:image" content="<%= settings.logo %>">
