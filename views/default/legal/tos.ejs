    <title><%= settings.name %></title>
    <link rel="icon" type="image/png" href="<%= settings.logo %>">
    <% if (settings.captcha.enabled == true) { %>
    <script src="https://www.google.com/recaptcha/api.js" async defer></script>
    <% } %>
    <% if (settings.ads.adsense.enabled == true) { %>
      <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-<%= settings.ads.adsense.clientid %>"
     crossorigin="anonymous"></script>
      <% } %>
