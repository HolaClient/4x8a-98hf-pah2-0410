   <body>
    <!-- component -->
    <header class="header sticky top-0 shadow-md flex flex-wrap items-center justify-between px-4 py-2 sm:px-8" id="header">
        <!-- Logo -->
        <div class="flex items-center justify-center sm:justify-start w-full sm:w-auto">
          <img style="width: 45px;" src="<%= settings.logo %>">
          <span class="ml-2 font-semibold text-lg"><%= settings.name %></span><br>
        </div>
      
        <!-- Navigation -->
        <nav class="nav font-semibold text-lg">
          <ul class="flex items-center space-x-4">
            <li class="border-b-2 border-green-500 border-opacity-0 hover:border-opacity-100 hover:text-green-500 duration-200 cursor-pointer active">
              <a href="../pp">Privacy Policy</a>
            </li>
            <li class="border-b-2 border-green-500 border-opacity-0 hover:border-opacity-100 hover:text-green-500 duration-200 cursor-pointer">
              <a href="../tos">TOS</a>
            </li>
            <li class="border-b-2 border-green-500 border-opacity-0 hover:border-opacity-100 hover:text-green-500 duration-200 cursor-pointer">
              <a href="../cookies">Cookies</a>
            </li>
          </ul>
        </nav>
      </header>
      
      <script>
        window.addEventListener('scroll', function() {
          var header = document.getElementById('header');
          if (window.pageYOffset > 0) {
            header.classList.add('bg-black');
          } else {
            header.classList.remove('bg-black');
          }
        });
      </script>
      
