    <script>
      async function getUptime() {
        const uptime = document.getElementById("uptime");
        const response = await fetch("/uptime");
        const json = await response.json();
        let html = "";
        json.uptime.forEach((entry) => {
          const [time, status] = entry.split(" - ");
          html += `<p text-white>${time} - ${status}</p>`;
        });
        uptime.innerHTML = html;
      }
      getUptime();
      setInterval(getUptime, 5000);
    </script>
            
