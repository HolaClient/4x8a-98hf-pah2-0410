<%- include('./partials/head') %>

<body class="bg-zinc-950 relative items-center h-full">
    <div class="w-screen overflow-hidden">
        <div class="antialiased">
            <%- include('./partials/nav') %>
            <%- include('./partials/aside') %>
            <main
            class="p-4 md:ml-64 z-20 -mt-[58px] max-w-full pt-40 md:pt-20 max-h-full min-h-screen shadow-2xl shadow-black backdrop-blur-sm">
            <div id="content">
                <div class="flex space-x-4 items-center">
                    <div
                        class="bg-zinc-800/50 h-max w-max group-hover:bg-zinc-900/50 group-hover:border-zinc-700 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-xl p-3">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor"
                            class="w-6 h-6">
                            <path d="M12 7.5a2.25 2.25 0 1 0 0 4.5 2.25 2.25 0 0 0 0-4.5Z" />
                            <path fill-rule="evenodd"
                                d="M1.5 4.875C1.5 3.839 2.34 3 3.375 3h17.25c1.035 0 1.875.84 1.875 1.875v9.75c0 1.036-.84 1.875-1.875 1.875H3.375A1.875 1.875 0 0 1 1.5 14.625v-9.75ZM8.25 9.75a3.75 3.75 0 1 1 7.5 0 3.75 3.75 0 0 1-7.5 0ZM18.75 9a.75.75 0 0 0-.75.75v.008c0 .414.336.75.75.75h.008a.75.75 0 0 0 .75-.75V9.75a.75.75 0 0 0-.75-.75h-.008ZM4.5 9.75A.75.75 0 0 1 5.25 9h.008a.75.75 0 0 1 .75.75v.008a.75.75 0 0 1-.75.75H5.25a.75.75 0 0 1-.75-.75V9.75Z"
                                clip-rule="evenodd" />
                            <path
                                d="M2.25 18a.75.75 0 0 0 0 1.5c5.4 0 10.63.722 15.6 2.075 1.19.324 2.4-.558 2.4-1.82V18.75a.75.75 0 0 0-.75-.75H2.25Z" />
                        </svg>
                    </div>
                    <div class="flex flex-col justify-between w-full">
                        <h1 class="text-gray-200 font-semibold text-xl"><%= lang.servers.title %></h1>
                        <span class="text-gray-300 text-md"><%= lang.servers.titleText %></span>
                    </div>
                </div>
                <div class="w-full mt-4 grid grid-cols-1 md:grid-cols-3 gap-4">
                    <div class="grid grid-cols-1 gap-4 col-span-2 w-full">
                        <div class="rounded-xl bg-zinc-900 border border-zinc-800/80 h-max p-2">
                            <h1 class="text-gray-200 text-xl text-center w-full">
                                Link based earning
                            </h1>
                            <div class="grid grid-cols-1 mt-2 md:grid-cols-3 gap-4">
                                <div
                                    class="bg-zinc-800/50 hover:bg-zinc-900 hover:duration-300 duration-300 cursor-pointer shadow rounded-xl p-4 flex items-center space-x-2 w-full border border-zinc-800/80">
                                    <img src="https://s3-eu-west-1.amazonaws.com/tpd/logos/60f32c3fa0f0500001ab7c87/0x0.png"
                                        class="w-12 h-auto rounded-xl">
                                    <h1 class="text-gray-300 text-xl">Work.ink</h1>
                                </div>
                                <div
                                    class="bg-zinc-800/50 hover:bg-zinc-900 hover:duration-300 duration-300 cursor-pointer shadow rounded-xl p-4 flex items-center space-x-2 w-full border border-zinc-800/80">
                                    <img src="data:image/png;base64,"
                                        class="w-12 h-auto rounded-xl">
                                    <h1 class="text-gray-300 text-xl">Adfoc.us</h1>
                                </div>
                                <div
                                    class="bg-zinc-800/50 hover:bg-zinc-900 hover:duration-300 duration-300 cursor-pointer shadow rounded-xl p-4 flex items-center space-x-2 w-full border border-zinc-800/80">
                                    <img src="https://spitzen-arbeitgeber.de/wp-content/uploads/2023/08/FINAL-Linkvertise-Icon.png"
                                        class="w-12 h-auto rounded-xl">
                                    <h1 class="text-gray-300 text-xl">Linkvertise</h1>
                                </div>
                            </div>
                        </div>
                        <div class="rounded-xl bg-zinc-900 border border-zinc-800/80 h-max p-2">
                            <h1 class="text-gray-200 text-xl text-center w-full">
                                Discord: Join For Coins
                            </h1>
                            <div class="grid grid-cols-1 mt-2 md:grid-cols-3 gap-4">
                                <div
                                    class="bg-sky-800/30 hover:bg-zinc-900 hover:duration-300 duration-300 cursor-pointer shadow rounded-xl p-4 flex items-center space-x-2 w-full border border-sky-800/80">
                                    <img src="https://cdn.holaclient.tech/logo.png" class="w-12 h-auto rounded-xl">
                                    <div class="flex flex-col">
                                        <h1 class="text-sky-300 text-lg">HolaClient</h1>
                                        <span class="text-gray-400">500 coins</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="rounded-xl bg-zinc-900 border border-zinc-800/80 h-max p-2">
                            <h1 class="text-gray-200 text-xl text-center w-full">
                                GitHub: Star For Coins
                            </h1>
                            <div class="grid h-max grid-cols-1 mt-2 md:grid-cols-3 gap-4">
                                <div
                                    class="bg-sky-800/30 hover:bg-zinc-900 hover:duration-300 duration-300 cursor-pointer shadow rounded-xl p-4 flex items-center space-x-2 w-full border border-sky-800/80">
                                    <img src="https://cdn.holaclient.tech/logo.png" class="w-12 h-auto rounded-xl">
                                    <div class="flex flex-col">
                                        <h1 class="text-sky-300 text-lg">HolaClient</h1>
                                        <span class="text-gray-400">500 coins</span>
                                    </div>
                                </div>
                                <div
                                class="bg-sky-800/30 hover:bg-zinc-900 hover:duration-300 duration-300 cursor-pointer shadow rounded-xl p-4 flex items-center space-x-2 w-full border border-sky-800/80">
                                <img src="https://cdn.holaclient.tech/logo.png"
                                    class="w-12 h-auto rounded-xl">
                                <div class="flex flex-col">
                                    <h1 class="text-sky-300 text-lg">HolaClient legacy</h1>
                                    <span class="text-gray-400">500 coins</span>
                                </div>
                            </div>
                            </div>
                        </div>
                    </div>
                    <div class="grid h-max grid-cols-1 gap-4 w-full">
                        <div class="rounded-xl bg-zinc-900 border border-zinc-800/80 p-2 h-max">
                            <h1 class="text-gray-200 text-xl text-center w-full">
                                Leaderboard
                            </h1>
                            <div class="flex flex-col mt-2 w-full">
                                <div
                                    class="bg-zinc-800/50 hover:bg-zinc-900 h-max hover:duration-300 duration-300 justify-between cursor-pointer shadow rounded-xl p-2 flex items-center space-x-2 w-full border border-zinc-800/80">
                                    <div class="flex items-center space-x-2">
                                        <h1 class="text-amber-300 text-xl">#1</h1>
                                        <img src="https://cdn.discordapp.com/avatars/863452735998656512/a_2990babbb1ca4fe3242456e5720990a6"
                                            class="w-12 rounded-xl">
                                        <h1 class="text-gray-200 text-xl">CR072</h1>
                                    </div>
                                    <h1 class="text-gray-200 text-lg">1000 <span class="text-gray-400">coins</span>
                                    </h1>
                                </div>
                            </div>
                        </div>
                        <div class="rounded-xl bg-zinc-900 border border-zinc-800/80 p-2 h-max">
                            <h1 class="text-gray-200 text-xl text-center w-full">
                                AFK
                            </h1>
                            <div class="flex flex-col mt-2 w-full">
                                <div
                                    class="bg-zinc-800/50 hover:bg-zinc-900 h-max hover:duration-300 duration-300 justify-center cursor-pointer shadow rounded-xl p-2 flex items-center space-x-2 w-full border border-zinc-800/80">
                                    <p class="text-gray-300">You are currently earning 5 coins per minute. In this
                                        session, you have earned 10 coins, bringing your total to 1010 coins.</p>
                                </div>
                            </div>
                        </div>
                        <div class="rounded-xl bg-zinc-900 border border-zinc-800/80 p-2 h-max">
                            <h1 class="text-gray-200 text-xl text-center w-full">
                                Redeem Coupons
                            </h1>
                            <div class="flex flex-col mt-2 w-full">
                                <input placeholder="HOLACLIENTX"
                                    class="bg-zinc-950 text-gray-200 placeholder-gray-400 outline-none focus:bg-zinc-950/80 focus:duration-300 duration-300 rounded-xl p-2 w-full border border-zinc-800/80">
                                    <button class="bg-zinc-950 mt-2 relative p-2 text-gray-200 outline-none border border-zinc-800/80 rounded-xl w-full">
                                        Redeem
                                    </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>
            <%- include('./partials/footer') %>
        </div>
    </div>
    </div>
</body>
</html>