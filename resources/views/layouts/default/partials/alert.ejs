                                                                                                                                                      toastr.warning('Please disable your AD blocker!', 'Warning');
                                                                                                                                                    }
                                                                                                                                                    showNotification()
                                                                                                                                                  </script>
                                                                                                                                                  <% }
                                                                                                                                                    %>
                                                                                                                                                    <% }
                                                                                                                                                      %>
                                                                                                                                                      <% if
                                                                                                                                                        (req.query.success)
                                                                                                                                                        {
                                                                                                                                                        %>
                                                                                                                                                        <script>
                                                                                                                                                          function showNotification() {
                                                                                                                                                            toastr.success('Operation Successful!', 'Success!');
                                                                                                                                                          }
                                                                                                                                                          showNotification()
                                                                                                                                                        </script>
                                                                                                                                                        <% }
                                                                                                                                                          %>
                                                                                                                                                          <% if
                                                                                                                                                            (req.query.err)
                                                                                                                                                            {
                                                                                                                                                            %>
