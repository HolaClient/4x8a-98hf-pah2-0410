                                                                                                                                                              (req.query.err=="MISSINGFIELDS"
                                                                                                                                                              )
                                                                                                                                                              {
                                                                                                                                                              %>
                                                                                                                                                              <script>
                                                                                                                                                                function showNotification() {
                                                                                                                                                                  toastr.error('Please fill-up the fields!', 'Error');
                                                                                                                                                                }
                                                                                                                                                                showNotification()
                                                                                                                                                              </script>
                                                                                                                                                              <% }
                                                                                                                                                                else
                                                                                                                                                                if
                                                                                                                                                                (req.query.err=="CANNOTGIFTYOURSELF"
                                                                                                                                                                )
                                                                                                                                                                {
                                                                                                                                                                %>
                                                                                                                                                                <script>
                                                                                                                                                                  function showNotification() {
                                                                                                                                                                    toastr.error('You cannot gift yourself!', 'Error');
                                                                                                                                                                  }
                                                                                                                                                                  showNotification()
                                                                                                                                                                </script>
                                                                                                                                                                <% }
                                                                                                                                                                  else
                                                                                                                                                                  if
                                                                                                                                                                  (req.query.err=="CANTAFFORD"
                                                                                                                                                                  )
                                                                                                                                                                  {
                                                                                                                                                                  %>
                                                                                                                                                                  <script>
                                                                                                                                                                    function showNotification() {
                                                                                                                                                                      toastr.error('You cannot afford this!', 'Error');
                                                                                                                                                                    }
                                                                                                                                                                    showNotification()
                                                                                                                                                                  </script>
                                                                                                                                                                  <% }
                                                                                                                                                                    else
                                                                                                                                                                    if
                                                                                                                                                                    (req.query.err=="USERDOESNTEXIST"
                                                                                                                                                                    )
                                                                                                                                                                    {
                                                                                                                                                                    %>
                                                                                                                                                                    <script>
                                                                                                                                                                      function showNotification() {
                                                                                                                                                                        toastr.error('We could not find an account with that ID!', 'Error');
                                                                                                                                                                      }
                                                                                                                                                                      showNotification()
                                                                                                                                                                    </script>
                                                                                                                                                                    <% }
                                                                                                                                                                      else
                                                                                                                                                                      if
                                                                                                                                                                      (req.query.err=="TOOLOWCOINS"
                                                                                                                                                                      )
                                                                                                                                                                      {
                                                                                                                                                                      %>
                                                                                                                                                                      <script>
                                                                                                                                                                        function showNotification() {
                                                                                                                                                                          toastr.error('You should at least gift more than 1 coins!', 'Error');
                                                                                                                                                                        }
                                                                                                                                                                        showNotification()
                                                                                                                                                                      </script>
                                                                                                                                                                      <% }
                                                                                                                                                                        %>
                                                                                                                                                                        <% }
                                                                                                                                                                          %>

                                                                                                                                                                          <% if
                                                                                                                                                                            (req.query.err=="MISSINGID"
                                                                                                                                                                            )
                                                                                                                                                                            {
                                                                                                                                                                            %>
                                                                                                                                                                            <script>
                                                                                                                                                                              function showNotification() {
                                                                                                                                                                                toastr.error('Missing ID', 'Error');
                                                                                                                                                                              }
                                                                                                                                                                              showNotification()
                                                                                                                                                                            </script>
                                                                                                                                                                            <% }
                                                                                                                                                                              else
                                                                                                                                                                              if
                                                                                                                                                                              (req.query.err=="CREATECOUPONINVALIDCHARACTERS"
                                                                                                                                                                              )
                                                                                                                                                                              {
                                                                                                                                                                              %>
                                                                                                                                                                              <script>
                                                                                                                                                                                function showNotification() {
                                                                                                                                                                                  toastr.warning('Coupon code has invalid characters', 'Warning');
                                                                                                                                                                                }
                                                                                                                                                                                showNotification()
                                                                                                                                                                              </script>
                                                                                                                                                                              <% }
                                                                                                                                                                                else
                                                                                                                                                                                if
                                                                                                                                                                                (req.query.err=="REVOKECOUPONCANNOTFINDCODE"
                                                                                                                                                                                )
                                                                                                                                                                                {
                                                                                                                                                                                %>
                                                                                                                                                                                <script>
                                                                                                                                                                                  function showNotification() {
                                                                                                                                                                                    toastr.error('Cannot find a coupon with this code!', 'Error');
                                                                                                                                                                                  }
                                                                                                                                                                                  showNotification()
                                                                                                                                                                                </script>
                                                                                                                                                                                <% }
                                                                                                                                                                                  else
                                                                                                                                                                                  if
                                                                                                                                                                                  (req.query.err=="MISSINGVARIABLES"
                                                                                                                                                                                  )
                                                                                                                                                                                  {
                                                                                                                                                                                  %>
                                                                                                                                                                                  <script>
                                                                                                                                                                                    function showNotification() {
                                                                                                                                                                                      toastr.warning('Please Fill up all fields!', 'Warning!');
                                                                                                                                                                                    }
                                                                                                                                                                                    showNotification()
                                                                                                                                                                                  </script>
                                                                                                                                                                                  <% }
                                                                                                                                                                                    else
                                                                                                                                                                                    if
                                                                                                                                                                                    (req.query.err=="IP"
                                                                                                                                                                                    )
                                                                                                                                                                                    {
                                                                                                                                                                                    %>
                                                                                                                                                                                    <script>
                                                                                                                                                                                      function pass() {
                                                                                                                                                                                        const urlParams = new URLSearchParams(window.location.search);
                                                                                                                                                                                        const ip = urlParams.get('ip');

                                                                                                                                                                                        if (ip) {
                                                                                                                                                                                          Swal.fire({
                                                                                                                                                                                            title: 'Success',
                                                                                                                                                                                            text: `IP: ${decodeURIComponent(ip)}`,
                                                                                                                                                                                            icon: 'success'
                                                                                                                                                                                          });
                                                                                                                                                                                        }
                                                                                                                                                                                      }

                                                                                                                                                                                      pass();
                                                                                                                                                                                    </script>
                                                                                                                                                                                    <% }
                                                                                                                                                                                      %>
                                                                                                                                                                                      <% if
                                                                                                                                                                                        (req.query.err=="CREATECOUPONEMPTY"
                                                                                                                                                                                        )
                                                                                                                                                                                        {
                                                                                                                                                                                        %>
                                                                                                                                                                                        <script>
                                                                                                                                                                                          function showNotification() {
                                                                                                                                                                                            toastr.warning('Please provide a coupon code!', 'Warning!');
                                                                                                                                                                                          }
                                                                                                                                                                                          showNotification()
                                                                                                                                                                                        </script>
                                                                                                                                                                                        </div>
                                                                                                                                                                                        <% }
                                                                                                                                                                                          else
                                                                                                                                                                                          if
                                                                                                                                                                                          (req.query.err==="CREATECOUPONLESSTHANONE"
                                                                                                                                                                                          )
                                                                                                                                                                                          {
                                                                                                                                                                                          %>
                                                                                                                                                                                          <script>
                                                                                                                                                                                            function showNotification() {
                                                                                                                                                                                              toastr.warning('Cannot create coupon with negative numbers!', 'Warning!');
                                                                                                                                                                                            }
                                                                                                                                                                                            showNotification()
                                                                                                                                                                                          </script>
                                                                                                                                                                                          <% }
                                                                                                                                                                                            if
                                                                                                                                                                                            (req.query.err==='INVALIDID'
                                                                                                                                                                                            )
                                                                                                                                                                                            {
                                                                                                                                                                                            %>
                                                                                                                                                                                            <script>
                                                                                                                                                                                              function showNotification() {
                                                                                                                                                                                                toastr.error('Could not find an account with this Email!', 'Error!');
                                                                                                                                                                                              }
                                                                                                                                                                                              showNotification()
                                                                                                                                                                                            </script>
                                                                                                                                                                                            <% }
                                                                                                                                                                                              if
                                                                                                                                                                                              (req.query.err=="RAMSIZE"
                                                                                                                                                                                              ||
                                                                                                                                                                                              req.query.err=="DISKSIZE"
                                                                                                                                                                                              ||
                                                                                                                                                                                              req.query.err=="CPUSIZE"
                                                                                                                                                                                              ||
                                                                                                                                                                                              req.query.err=="SERVERSIZE"
                                                                                                                                                                                              )
                                                                                                                                                                                              {
                                                                                                                                                                                              %>
                                                                                                                                                                                              <script>
                                                                                                                                                                                                function showNotification() {
                                                                                                                                                                                                  toastr.error('Cannot create coupon with negative numbers!', 'Error!');
                                                                                                                                                                                                }
                                                                                                                                                                                                showNotification()
                                                                                                                                                                                              </script>
                                                                                                                                                                                              <% }
                                                                                                                                                                                                if
                                                                                                                                                                                                (req.query.code)
                                                                                                                                                                                                {
                                                                                                                                                                                                %>
                                                                                                                                                                                                <script>
                                                                                                                                                                                                  function showNotification() {
                                                                                                                                                                                                    toastr.success('Successfully created a coupon: <%= req.query.code %>', 'Success');
                                                                                                                                                                                                  }
                                                                                                                                                                                                  showNotification()
                                                                                                                                                                                                </script>
                                                                                                                                                                                                <% }
                                                                                                                                                                                                  if
                                                                                                                                                                                                  (req.query.err==='NOIP'
                                                                                                                                                                                                  )
                                                                                                                                                                                                  {
                                                                                                                                                                                                  %>
                                                                                                                                                                                                  <script>
                                                                                                                                                                                                    function showNotification() {
                                                                                                                                                                                                      toastr.warning('<%= settings.name %> do not support storing user IP in database!', 'Warning!');
                                                                                                                                                                                                    }
                                                                                                                                                                                                    showNotification()
                                                                                                                                                                                                  </script>
                                                                                                                                                                                                  <% }
                                                                                                                                                                                                    if
                                                                                                                                                                                                    (req.query.err==='REMOVEACCOUNT'
                                                                                                                                                                                                    )
                                                                                                                                                                                                    {
                                                                                                                                                                                                    %>
                                                                                                                                                                                                    <script>
                                                                                                                                                                                                      function showNotification() {
                                                                                                                                                                                                        toastr.success('Successfully removed that account from our database!', 'Success');
                                                                                                                                                                                                      }
                                                                                                                                                                                                      showNotification()
                                                                                                                                                                                                    </script>
                                                                                                                                                                                                    <% }
