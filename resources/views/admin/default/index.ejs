<%- include('./partials/head') %>

<body class="bg-zinc-950 relative items-center h-full">
    <div class="w-screen overflow-hidden">
        <div class="antialiased">
            <%- include('./partials/nav') %>
            <%- include('./partials/aside') %>
            <main
            class="p-4 md:ml-64 z-20 -mt-[58px] max-w-full pt-40 md:pt-20 max-h-full min-h-screen shadow-2xl shadow-black backdrop-blur-sm">
            <div id="content">
                <div class="flex space-x-4 items-center">
                    <div
                        class="bg-zinc-800/50 h-max w-max group-hover:bg-zinc-900/50 group-hover:border-zinc-700 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-xl p-3">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor"
                            class="w-6 h-6">
                            <path
                                d="M11.47 3.841a.75.75 0 0 1 1.06 0l8.69 8.69a.75.75 0 1 0 1.06-1.061l-8.689-8.69a2.25 2.25 0 0 0-3.182 0l-8.69 8.69a.75.75 0 1 0 1.061 1.06l8.69-8.689Z" />
                            <path
                                d="m12 5.432 8.159 8.159c.03.03.06.058.091.086v6.198c0 1.035-.84 1.875-1.875 1.875H15a.75.75 0 0 1-.75-.75v-4.5a.75.75 0 0 0-.75-.75h-3a.75.75 0 0 0-.75.75V21a.75.75 0 0 1-.75.75H5.625a1.875 1.875 0 0 1-1.875-1.875v-6.198a2.29 2.29 0 0 0 .091-.086L12 5.432Z" />
                        </svg>
                    </div>
                    <div class="flex flex-col justify-between w-full">
                        <h1 class="text-gray-200 font-semibold text-xl"><%= lang.admin.home.title %></h1>
                        <span class="text-gray-300 text-md"><%= lang.admin.home.titleText %></span>
                    </div>
                </div>
                <div class="justify-between w-full mt-4 items-center">
                    <div class="grid grid-cols-2 md:grid-cols-4 lg:grid-cols-5 xl:grid-cols-6 w-full gap-4">
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">1</h1> <span
                                class="text-gray-300"><%= lang.admin.home.servers %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">2</h1> <span
                                class="text-gray-300"><%= lang.admin.home.users %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">1</h1> <span
                                class="text-gray-300"><%= lang.admin.home.queue %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">1</h1> <span
                                class="text-gray-300"><%= lang.admin.home.nodes %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">1</h1> <span
                                class="text-gray-300"><%= lang.admin.home.subdomains %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">100</h1> <span
                                class="text-gray-300">Coins Circulating</span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">1</h1> <span
                                class="text-gray-300"><%= lang.admin.home.invoices %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">1</h1> <span
                                class="text-gray-300"><%= lang.admin.home.tickets %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">$1</h1> <span
                                class="text-gray-300"><%= lang.admin.home.revenue %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">100%</h1> <span
                                class="text-gray-300"><%= lang.admin.home.memory %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">100%</h1> <span
                                class="text-gray-300"><%= lang.admin.home.disk %></span>
                        </div>
                        <div
                            class="flex flex-col justify-center overflow-hidden items-center bg-zinc-900/50 border border-zinc-800/80 backdrop-blur-3xl shadow text-gray-400 rounded-3xl p-3">
                            <h1 class="text-gray-200 mt-2 text-3xl font-karla">1</h1> <span
                                class="text-gray-300"><%= lang.admin.home.newUsers %></span>
                        </div>
                    </div>
                    <div class="grid grid-cols-1 md:grid-cols-3 w-full gap-4 mt-4">
                        <div class="bg-zinc-900/50 h-max relative border border-zinc-800/80 rounded-xl p-2 w-full">
                            <h1 class="text-gray-100 text-xl"><%= lang.admin.home.purgeServers %>:</h1>
                            <p class="text-gray-400"><%= lang.admin.home.warning %></p>
                            <div class="mt-2">
                                <h1 class="text-gray-300"><%= lang.admin.home.keyword %>:</h1>
                                <input id="" type="text" value="[Active]" class="p-2 bg-zinc-950 outline-none border border-zinc-800/80 rounded-xl w-full text-gray-300">
                            </div>
                            <button class="p-2 bg-rose-700/50 mt-2 w-full border border-rose-700 rounded-xl text-rose-400">
                                Purge
                            </button>
                        </div>
                        <div class="bg-zinc-900/50 h-max relative border border-zinc-800/80 rounded-xl p-2 w-full">
                            <h1 class="text-gray-100 text-xl"><%= lang.admin.home.purgeSuspended %>:</h1>
                            <p class="text-gray-400"><%= lang.admin.home.warning %></p>
                            <div class="mt-2">
                                <h1 class="text-gray-300"><%= lang.admin.home.suspended %>:</h1>
                                <input id="" disabled type="text" value="1" class="p-2 bg-zinc-950 outline-none border border-zinc-800/80 rounded-xl w-full text-gray-300">
                            </div>
                            <button class="p-2 bg-rose-700/50 mt-2 w-full border border-rose-700 rounded-xl text-rose-400">
                                Purge
                            </button>
                        </div>
                        <div class="bg-zinc-900/50 h-max relative border border-zinc-800/80 rounded-xl p-2 w-full">
                            <h1 class="text-gray-100 text-xl"><%= lang.admin.home.purgeInactive %>:</h1>
                            <p class="text-gray-400"><%= lang.admin.home.warning %></p>
                            <div class="mt-2">
                                <h1 class="text-gray-300"><%= lang.admin.home.period %>:</h1>
                                <select id="" type="text" class="p-2 bg-zinc-950 outline-none border border-zinc-800/80 rounded-xl w-full text-gray-300">
                                    <option value="">7d</option>
                                    </select>
                            </div>
                            <button class="p-2 bg-rose-700/50 mt-2 w-full border border-rose-700 rounded-xl text-rose-400">
                                Purge
                            </button>
                        </div>
                        <div class="bg-zinc-900/50 h-max relative border border-zinc-800/80 rounded-xl p-2 w-full">
                            <h1 class="text-gray-100 text-xl"><%= lang.admin.home.purgeServersCache %>:</h1>
                            <p class="text-gray-400"><%= lang.admin.home.warning1 %></p>
                            <button class="p-2 bg-rose-700/50 mt-2 w-full border border-rose-700 rounded-xl text-rose-400">
                                Purge
                            </button>
                        </div>
                        <div class="bg-zinc-900/50 h-max relative border border-zinc-800/80 rounded-xl p-2 w-full">
                            <h1 class="text-gray-100 text-xl"><%= lang.admin.home.purgeCache %>:</h1>
                            <p class="text-gray-400"><%= lang.admin.home.warning2 %></p>
                            <button class="p-2 bg-rose-700/50 mt-2 w-full border border-rose-700 rounded-xl text-rose-400">
                                Purge
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </main>
            <%- include('./partials/footer') %>
        </div>
    </div>
    </div>
</body>
</html>