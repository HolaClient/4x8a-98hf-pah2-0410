{
    "properties": {
        "economy": {
            "key": "coins-$discord.id",
            "requires": [
                "discord",
                "discord.id"
            ],
            "value": {
                "type": "number",
                "targets": {
                    "": {
                        "key": "coins",
                        "value": "$value"
                    }
                }
            }
        },
        "resources": {
            "key": "extra-$discord.id",
            "requires": [
                "discord",
                "discord.id"
            ],
            "value": {
                "type": "object",
                "targets": {
                    "ram": {
                        "key": "memory",
                        "value": "$value"
                    }
                },
                "extends": [
                    {
                        "key": "allocations",
                        "value": "$hc.resources.default.allocations"
                    },
                    {
                        "key": "databases",
                        "value": "$hc.resources.default.databases"
                    },
                    {
                        "key": "backups",
                        "value": "$hc.resources.default.backups"
                    }
                ]
            }
        }
    }
}